[{"@context":"http://schema.org/"},{"@type":["DefinedTermSet","Consortium","Project"],"@id":"https://EcmaTC53.github.io/lexicon","name":"ECMA TC-53 Terms"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Application","name":"Application","description":"In the context of TC-53, an application is a software program designed to perform a specific function on a specific device. Applications may import libraries and providers in the form of modules.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Built-in","name":"Built-in","description":"Built-ins are javascript modules that are bundled with the host.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://github.com/tc39/proposal-javascript-standard-library/"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Data Accuracy","name":"Data Accuracy","description":"How close a data measurement is to it's theoretical expected output.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Data Provenance","name":"Data Provenance","description":"Knowing where data came from and how it arrived.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Data Timeliness","name":"Data Timeliness","description":"A measure of data timeliness. For example, how long did that \"read\" command take to execute? On embedded systems this could be a few ns. On a cloud based device it could be several seconds.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Data Trustworthiness","name":"Data Trustworthiness","description":"The combination of data provenance, timeliness an accuracy.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#External provider","name":"External provider","description":"A downloadable module that provides a surface API that matches the GPIO API specified by TC-53.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Implementor","name":"Implementor","description":"One who authors a provider.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Internal provider","name":"Internal provider","description":"A built-in module that provides a surface API that matches the GPIO API specified by TC-53.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#IOCP","name":"IOCP","description":"See I/O Class Pattern","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#I/O Class Pattern","name":"I/O Class Pattern","description":"","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Library","name":"Library","description":"A library provides a surface API that abstracts away the GPIO API. For example, an LED library could provide an API with the methods on(), off(), blink() and toggle() that abstract away an application developers need to interact directly with a digital IO instance.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Maximally Minimal","name":"Maximally Minimal","description":"The supposition that specifying a minimal API surface for base classes allows for greatest value realization and a reduced risk of painting oneself into a corner. A concept based on conversations around EcmaScript classes.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Module","name":"Module","description":"","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Normative","name":"Normative","description":"Things that have been standardized and must be followed as a rule.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Normative"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Protocol","name":"Protocol","description":"A system of rules that define how data is exchanged between systems.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Protocol"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Provider","name":"Provider","description":"A provider is a module that provides a surface API that matches the GPIO API specified by TC-53.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Sleepability","name":"Sleepability","description":"","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Third-Party Libraries","name":"Third-Party Libraries","description":"","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#W3C Sensor API","name":"W3C Sensor API","description":"","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Wake-ability","name":"Wake-ability","description":"","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Web Things","name":"Web Things","description":"","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"}]