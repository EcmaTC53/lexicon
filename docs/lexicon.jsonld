[{"@context":"http://schema.org/"},{"@type":["DefinedTermSet","Consortium","Project"],"@id":"https://EcmaTC53.github.io/lexicon","name":"ECMA TC-53 Terms"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#1-wire","name":"1-wire","description":"a serial protocol using a single data line plus ground reference for communication.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://www.maximintegrated.com/en/app-notes/index.mvp/id/1796"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Actuator","name":"Actuator","description":"A device that is reponsible for moving some part of a machine (eg a servo or motor).","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Address","name":"Address","description":"An identifier for interfacing with a specific component, device, or board.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Analog","name":"Analog","description":"Systems with a continuosly variable signal.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Analog-to-Digital Converter","name":"Analog-to-Digital Converter","description":"Converts an analog signal to a digital value.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Application","name":"Application","description":"In the context of TC-53, an application is a software program designed to perform a specific function on a specific device. Applications may import libraries and providers in the form of modules.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Application Programming Interface (API)","name":"Application Programming Interface (API)","description":"A set of features and rules that exist inside a software program enabling interaction with it through software.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/API"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Arguments","name":"Arguments","description":"An Array-like object accessible inside functions that contains the values of the arguments passed to that function.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Functions/arguments"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Asynchronous","name":"Asynchronous","description":"A communication environment where each party receives and processes messages when possible rather than immediately.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Asynchronous"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Baud Rate","name":"Baud Rate","description":"Rate at which information in transferred. Measured in bytes per second.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Binding","name":"Binding","description":"The value of a function's this attribute, which can vary with how the function was called. See \"This\".","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Blocking","name":"Blocking","description":"Code which runs outside of the current execution context but forces the context to wait.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Buffer","name":"Buffer","description":"A storage in physical memory used to temporarily store data while it is being transferred from one place to another.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Buffer"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Built-in","name":"Built-in","description":"Built-ins are javascript modules that are bundled with the host.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://github.com/tc39/proposal-javascript-standard-library/"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Bus","name":"Bus","description":"A communications system that transfers data. A \"Bus\" includes hardware, software and the protocol.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Button","name":"Button","description":"A mechanism that completes an electrical circuit when pressed.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Byte","name":"Byte","description":"A unit of data consisting of 8 binary bits","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Callback","name":"Callback","description":"A callback function is passed as an argument to another function and is exectued when some process is completed.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#CANbus","name":"CANbus","description":"The Controller Area Network protocol enables communication between a network of devices. CAN is widley used in automobile electronics.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Class","name":"Class","description":"Class is a template definition of an object's properties and methods.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Class"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Clock Signal","name":"Clock Signal","description":"A signal that oscillates between high and low, that is used like a metronome to control circuit timing.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Closure","name":"Closure","description":"The binding which defines the scope of execution.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Closure"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Compartments","name":"Compartments","description":"Compartments are used to store separate JavaScript heaps improving GC performance and adding security.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Compile Time Function Execution","name":"Compile Time Function Execution","description":"When the arguments to a pure function are known at compile time the compiler can create more highly optimized code vs compilation at run time.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Composable","name":"Composable","description":"Functions that can be combined with one or more other functions. Composable functions require one input argument and one output value.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Constructor","name":"Constructor","description":"The constructor initializes an object and can provide access to its private information. ","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Constructor"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Current","name":"Current","description":"The rate of flow of an electric charge.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Data Accuracy","name":"Data Accuracy","description":"How close a data measurement is to it's theoretical expected output.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Data Provenance","name":"Data Provenance","description":"Knowing where data came from and how it arrived.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Data Timeliness","name":"Data Timeliness","description":"A measure of data timeliness. For example, how long did that \"read\" command take to execute? On embedded systems this could be a few ns. On a cloud based device it could be several seconds.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Data Trustworthiness","name":"Data Trustworthiness","description":"The combination of data provenance, timeliness an accuracy.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Dependency Injection","name":"Dependency Injection","description":"Creating and passing in an obect's dependencies rather that having that object create it's own dependencies.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Digital","name":"Digital","description":"Capable of having just two states, high and low.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Driver","name":"Driver","description":"Software or a circuit used to control another decice.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Duty cycle","name":"Duty cycle","description":"The percentage of time that a PWM signal is high or on.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Embedded Device","name":"Embedded Device","description":"A single purpose computing system wholly contained within another device or product.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Encapsulation","name":"Encapsulation","description":"The packing of data and functions into one component and then controlling access to that component.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Encapsulation"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#ES-Strict","name":"ES-Strict","description":"See \"Strict Mode\"","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Events","name":"Events","description":"An event which takes place on an object or element. Event targets can be configured to listen for certain events and call an Event Handler when the happen.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Expander","name":"Expander","description":"A device that provides addition inputs and/or outputs.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Extends","name":"Extends","description":"The extends keyword is used in class declarations or class expressions to create a class which is a child of another class.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Classes/extends"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Extensible Web Manifesto","name":"Extensible Web Manifesto","description":"A set of principles that guides how web standards committees create and prioritize new features. These principles include:  <ul>    <li>Focus on adding new low-level capabilities to the web platform that are secure and efficient.</li>    <li>Expose low-level capabilities that explain existing features, such as HTML and CSS, allowing authors to understand and replicate them.</li>    <li>Develop, describe and test new high-level features in JavaScript, and allow web developers to iterate on them before they become standardized. This creates a virtuous cycle between standards and developers.</li>    <li>Prioritize efforts that follow these recommendations and deprioritize and refocus those which do not.</li>  </ul>","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://extensiblewebmanifesto.org/"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#External provider","name":"External provider","description":"A downloadable module that provides a surface API that matches the GPIO API specified by TC-53.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Function","name":"Function","description":"A code snippet that can be called by other code or by itself.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Function"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#General Purpose Input/Output (GPIO)","name":"General Purpose Input/Output (GPIO)","description":"A digital signal pin that can be configured for input or output.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Generic Sensor API","name":"Generic Sensor API","description":"A framework for exposing sensor data to the Open Web Platform in a consistent way. ","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://www.w3.org/TR/generic-sensor/"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Harden","name":"Harden","description":"Tamper-proof an API surface (See SES).","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Hardware Abstraction Layer (HAL)","name":"Hardware Abstraction Layer (HAL)","description":"A software layer between the OS or host and the hardware that provides a standard interface for interacting with hardware components.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Host","name":"Host","description":"Provides the Javascript environment including the javascript engine, built-ins, and other objects.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#I2C","name":"I2C","description":"A communication bus standard that requires two lines and supports multiple, addressable devices on the same connection.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://www.i2c-bus.org/"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Immutable","name":"Immutable","description":"An object whose content cannot be changed.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Immutable"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Impedance","name":"Impedance","description":"The resistance  of a electrical circuit to current.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Implementor","name":"Implementor","description":"An implementor creates a provider. An implementor could be the creator of a host, or a 3rd party library.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Import","name":"Import","description":"The import statement will bring in bindings provided by external modules.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Instance","name":"Instance","description":"An object that has been created by a function constructor, class constructor, or function factory.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Instance"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Internet of Things (IoT)","name":"Internet of Things (IoT)","description":"The universe of common devices with connectivity that allows them to send and receive data over the Internet.      ","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Internal provider","name":"Internal provider","description":"A built-in module that provides a surface API that matches the GPIO API specified by TC-53.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Interrupt","name":"Interrupt","description":"An interrupt is an external signal that indicates some process should be executed immediately.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#I/O Class Pattern","name":"I/O Class Pattern","description":"The I/O Class Pattern describes a logical superclass for various hardware interface types (Digital, Analog, Serial, I2C, etc).","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://gist.github.com/phoddie/166c9c17b2f31d0beda9f2410a219268"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Javascript Object Notation (JSON)","name":"Javascript Object Notation (JSON)","description":"A data-interchange format that closely resembles a subset of JavaScript syntax.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/JSON"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Job","name":"Job","description":"A task to be executed asynchronously.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#JTAG","name":"JTAG","description":"JTAG is a common hardware interface that provides your computer with a way to communicate directly with the chips on a board.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://blog.senr.io/blog/jtag-explained"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Library","name":"Library","description":"A library provides a surface API that abstracts away the GPIO API. For example, an LED library could provide an API with the methods on(), off(), blink() and toggle() that abstract away an application developers need to interact directly with a digital IO instance.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Maximally Minimal","name":"Maximally Minimal","description":"The supposition that specifying a minimal API surface for base classes allows for greatest value realization and a reduced risk of painting oneself into a corner. A concept based on conversations around EcmaScript classes.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Method","name":"Method","description":"A function which is a property of an object.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Method"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Microcontroller (MCU)","name":"Microcontroller (MCU)","description":"A Microcontroller is a single integrated circuit with one or more CPUs, memory and programmable input/output.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Mixin","name":"Mixin","description":"A class or interface in which some or all of its methods and/or properties are unimplemented, requiring that another class or interface provide the missing implementations. Mixins extend a class.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Mixin"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Module","name":"Module","description":"Ecmascript code stored in an external file and imported via the ES6 import statement.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Statements/import"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Mutable","name":"Mutable","description":"A variable that can be changed.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Mutable"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Native bindings","name":"Native bindings","description":"Native bindings allow Ecmascript to call native code.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Non-blocking","name":"Non-blocking","description":"Code that does not block execution.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Normative","name":"Normative","description":"Things that have been standardized and must be followed as a rule.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Normative"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Parameter","name":"Parameter","description":"A named variable passed into a function.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Parameter"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Partial Evaluation","name":"Partial Evaluation","description":"When some arguments to a pure function are known at compile time the compiler can create more highly optimized code vs compilation at run time.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Pin bank  ","name":"Pin bank  ","description":"A group of input/output pins.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Polling","name":"Polling","description":"When code periodically checks the state of an input to see if a change has occurred.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Polymorphism","name":"Polymorphism","description":"The presentation of one interface for multiple data types.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Polymorphism"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Precompile","name":"Precompile","description":"See \"Partial Evaluation\" and \"Compile Time Function Execution\" ","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Promise","name":"Promise","description":"An object that's returned by a function that has not yet completed its work. ","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Promise"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Protocol","name":"Protocol","description":"A system of rules that define how data is exchanged between systems.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Protocol"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Provider","name":"Provider","description":"A provider is a module that provides a surface API that matches the GPIO API specified by TC-53.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Pull up/Pull down resisitor","name":"Pull up/Pull down resisitor","description":"A resistor used to ensure a known state for a signal. In an open circuit (eg a switch that is off, or button that is not pressed) half the circuit is in an indeterminate or \"floating\" state. A pull up or pull down resistor can address that.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Pulse Width Modulation (PWM)","name":"Pulse Width Modulation (PWM)","description":"Pulse width modulation is the regulation of a digital signal between on and off (duty cycle) over a fixed period of time (frequency) to create an averaged value voltage.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Reflection","name":"Reflection","description":"The ability of a program to inspect or modify its own behavior.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Register","name":"Register","description":"Locations in a device's memory which can be written to or read from. These memory locations may contain configuration settings or current state of the device.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Self Hosted","name":"Self Hosted","description":"~~~ Need help with this from tc53-2018-019 (Boston meeting) ~~~","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Secure EcmaScript","name":"Secure EcmaScript","description":"a frozen environment that removes programs abilities to interfere with each other.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://github.com/Agoric/SES"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Sensor","name":"Sensor","description":"A device that detects and responds to some type of input from the physical environment. ","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Serial","name":"Serial","description":"A bus that communicates using one bit at a time. RS-232, I2C and SPI are all examples of serial communication.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Serial Peripheral Interface (SPI)","name":"Serial Peripheral Interface (SPI)","description":"A synchronous serial communication protocol that support full duplex communication and multiple devices on a single bus. ","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Servomotor (Servo)","name":"Servomotor (Servo)","description":"A particular form of servomechanism that uses a potnetiometer or encoder to precisely position a motor.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Single Board Computer (SBC)","name":"Single Board Computer (SBC)","description":"A complete computer, including microprocessor, memory and I/O, built on a single circuit board. SBC's are usually designed to consume low power and offer a compact form factor.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Single-threaded","name":"Single-threaded","description":"One command is processed at a time.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Sleep","name":"Sleep","description":"Sleep mode in embeddable systems is also known as low-power or power-down mode. Current data is retained, the core processor is still running and the system can respond to \"wake\" events on interrupt or other I/O.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Stepper Motor","name":"Stepper Motor","description":"An electric motor that moves a set number of degrees per \"step\", a step being a change in the voltage applied to the devices coils (usually two). ","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Strict Mode","name":"Strict Mode","description":"A restricted variant of EcmaScript.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Strict_mode"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Subclass","name":"Subclass","description":"A class that inherits the attributes of another class.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Synchronous","name":"Synchronous","description":"Real-time communication where each party receives, processes and replies to messages instantly.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Glossary/Synchronous"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Tasks (W3C Tasks)","name":"Tasks (W3C Tasks)","description":"A set of instructions queued onto the event loop for exectution.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Third-Party Libraries","name":"Third-Party Libraries","description":"Providers, libraries, or frameworks provided by a developer other than the Provider developer or the application developer.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#This","name":"This","description":"The JavaScript context object in which the current code is executing.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://developer.mozilla.org/en-US/docs/Web/JavaScript/Reference/Operators/this"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Thread","name":"Thread","description":"An execution context.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Unit Tests","name":"Unit Tests","description":"Software testing against individual units of source code.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Universal Asynchronous Receiver/Transmitter (UART)","name":"Universal Asynchronous Receiver/Transmitter (UART)","description":"A device that controls the interface to attached serial devices.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#W3C Sensor API","name":"W3C Sensor API","description":"See \"Generic Sensor API\"","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Web Things","name":"Web Things","description":"A common data model and API for the Web of Things","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld","url":"https://iot.mozilla.org/wot/"},{"@type":"DefinedTerm","@id":"https://EcmaTC53.github.io/lexicon#Web Worker","name":"Web Worker","description":"An object the can run code in a seperate thread or execution context.","inDefinedTermSet":"https://EcmaTC53.github.io/lexicon/lexicon.jsonld"}]